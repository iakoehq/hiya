        </h1>
      {%- endif -%}
    {%- endif -%}

    {%- if section.settings.menu != blank -%}
      {%- if section.settings.menu_type_desktop == 'dropdown' -%}
      <nav class="navigation"{% if product.tags contains 'affiliate' %} style="display:none;"{% endif %}>
        <ul class="navigation__menu">
            {% for link in linklists[section.settings.menu].links %}
                {% if link.links != blank %}
                    <li>
                        <a href="{{ link.url }}" class="{% if link.active == true %}active{% endif %}">{{ link.title }}</a>
                        <ul class="submenu">
                            {% for childlink in link.links %}
                                <li>
                                    <a href="{{ childlink.url }}">{{ childlink.title }}</a>
                                </li>
                            {% endfor %}
                        </ul>
                    </li>
                {% else %}
                    <li>
                        {%- assign labelDowncase = link.title | downcase -%}
                        <a href="{{ link.url }}" class="{% if link.active == true %}active{% endif %} {% if labelDowncase  contains "bedtime" %}new_element{% endif %}">{{ link.title }}</a>
                    </li>
                {% endif %}
            {% endfor %}
        </ul>
    </nav>
    
      {%- elsif section.settings.menu_type_desktop != 'drawer' -%}
