{%- style -%}
  #mobileNavigation { 
    padding: 1.75rem 1rem 2.5rem;
  }
  header-drawer {
    justify-self: start;
    margin-left: -1rem;
  }
  .header__icon {
    width: 2.4rem;
  }

  #hide-me {
  display: none;
  }

  .cart-count-bubble {
    bottom: unset;
    top: 0.6rem;
    height: 1rem;
    width: 0.9rem;
    left: 1.6rem;
    color: #ffffff;
    font-size: 10px;
  }
  {%- if section.settings.sticky_header_type == 'reduce-logo-size' -%}
  .scrolled-past-header .header__heading-logo-wrapper {
    width: 75%;
  }
  {%- endif -%}

  .menu-drawer-container {
    display: flex;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
  }

  summary.list-menu__item {
    padding-right: 2.7rem;
  }

  .list-menu__item {
    display: flex;
    align-items: center;
    line-height: calc(1 + 0.3 / var(--font-body-scale));
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: calc(1 + 0.8 / var(--font-body-scale));
  }

  .section-header {
    position: sticky; /* This is for fixing a Safari z-index issue. PR #2147 */
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }
  
  summary.header__icon {
    position: relative;
  }

  span.hamburger__menu {
      display: block;
  }
  
  span.hamburger__menu {
      top: 56%;
      left: 50%;
      transform: translate(-50%, -50%);
      transition: background .3s;
  }
  
  span.hamburger__menu,
  span.hamburger__menu:before,
  span.hamburger__menu:after {
      position: absolute;
      height: 1.7px;
      width: 24px;
      background: #407AB9;
  }
  
  span.hamburger__menu:before,
  span.hamburger__menu:after {
      content: "";
      left: 0;
      transition: background .3s, transform .3s;
  }
  
  span.hamburger__menu:before {
      transform: translateY(-7px);
  }
  
  span.hamburger__menu:after {
      transform: translateY(7px);
  }
  
  .menu-opening span.hamburger__menu {
      background: transparent;
  }
  
  .menu-opening span.hamburger__menu:before,
  .menu-opening span.hamburger__menu:after {
      background: #407AB9;
  }
  
  .menu-opening span.hamburger__menu:before {
      transform: rotate(-45deg);
  }
  
  .menu-opening span.hamburger__menu:after {
      transform: rotate(45deg);
  }
  
  .menu-open header.header--has-menu {
      background-color: #fef8e9;
  }
  
  @media screen and (min-width: 750px) {
    .section-header {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }

  @media screen and (min-width: 1024px) {
  {%- if section.settings.menu_type_desktop != "drawer" -%}
    header-drawer {
      display: none;
    }
  {%- endif -%}
    .header {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}

